          android:layout_height="8dp"
          android:background="@drawable/drop_shadow_reverse" />

      <LinearLayout
        android:id="@+id/ll_content"
        android:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <!--Car info-->
        <android.support.v7.widget.CardView
          style="@style/ItemCardViewStyle"
          android:visibility="visible">

          <LinearLayout style="@style/Widget.CardContent">

            <TableLayout style="@style/ItemTableLayoutStyle">

              <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                  style="@style/ItemCardHeaderTextStyle"
                  android:text="@string/car_information"
                  android:textColor="@color/colorAccent2" />
              </TableRow>

              <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <View style="@style/ItemCardHeaderTextUnderLineStyle" />
              </TableRow>

              <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp">


                <TextView
                  android:text="@string/car_brand"
                  style="@style/ItemCardFieldTextStyle" />

                <TextView
                  android:id="@+id/tv_car_brand"
                  android:text="-"
                  style="@style/ItemCardFieldValueStyle"
                  android:textColor="@color/colorAccent2"/>

              </TableRow>
              <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp">

                <TextView
                  android:text="@string/car_model"
                  style="@style/ItemCardFieldTextStyle" />

                <TextView
                  android:id="@+id/tv_model"
                  android:text="-"
                  style="@style/ItemCardFieldValueStyle"
                  android:textColor="@color/colorAccent2"/>

              </TableRow>
              <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:paddingBottom="8dp">

                <TextView
                  android:text="@string/car_year"
                  style="@style/ItemCardFieldTextStyle" />

                <TextView
                  android:id="@+id/tv_year"
                  android:text="-"
                  style="@style/ItemCardFieldValueStyle"
                  android:textColor="@color/colorAccent2"/>

              </TableRow>

            </TableLayout>
          </LinearLayout>
        </android.support.v7.widget.CardView>

        <!--Car numbers-->
        <android.support.v7.widget.CardView
          android:visibility="visible"
          style="@style/ItemCardViewStyle">

          <LinearLayout style="@style/Widget.CardContent">

            <TableLayout style="@style/ItemTableLayoutStyle">

              <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                  style="@style/ItemCardHeaderTextStyle"
                  android:text="@string/car_license_plate"
                  android:textColor="@color/colorAccent2" />
              </TableRow>

              <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <View style="@style/ItemCardHeaderTextUnderLineStyle" />
              </TableRow>
              <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="0dp"
                android:layout_marginTop="8dp">


                <TextView
                  android:id="@+id/tv_numbers"
                  android:text="@string/license_plate"
                  style="@style/ItemCardFieldTextStyle"
                  android:textColor="@color/colorPrimary"/>

              </TableRow>
            </TableLayout>
          </LinearLayout>
        </android.support.v7.widget.CardView>

